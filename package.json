{
	"name": "vs-leet",

	"publisher": "abeshahsan",
	"displayName": "VS-Leet - LeetCode Integration",
	"description": "Complete LeetCode integration for VS Code - Browse, solve, test, and submit coding problems directly in your editor",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.90.0"
	},
	"categories": [
		"Programming Languages",
		"Education",
		"Other"
	],
	"keywords": [
		"leetcode",
		"algorithm",
		"interview",
		"coding",
		"practice",
		"competitive programming",
		"dsa",
		"data structures"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/abeshahsan/VS-Leet.git"
	},
	"bugs": {
		"url": "https://github.com/abeshahsan/VS-Leet/issues"
	},
	"license": "MIT",
	"icon": "resources/vs-leet-logo.png",
	"galleryBanner": {
		"color": "#1e1e1e",
		"theme": "dark"
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "vs-leet",
					"title": "VS-Leet",
					"icon": "resources/vs-leet-logo.svg"
				}
			]
		},
		"views": {
			"vs-leet": [
				{
					"id": "vs-leet.problemsView",
					"name": "Problems",
					"icon": "resources/vs-leet-logo.svg"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "vs-leet.problemsView",
				"contents": "Welcome to VS-Leet! Please sign in to browse and solve problems.\n[Sign In](command:vs-leet.signIn)",
				"when": "!vs-leet.loggedIn"
			}
		],
		"customEditors": [],
		"configuration": {
			"type": "object",
			"title": "VS-Leet",
			"properties": {
				"vs-leet.autoShow": {
					"type": "boolean",
					"default": false,
					"description": "Automatically show the problem browser when VS Code starts"
				},
				"vs-leet.solutionFolder": {
					"type": "string",
					"default": "Solutions",
					"description": "Folder name for storing solution files"
				},
				"vs-leet.defaultLanguage": {
					"type": "string",
					"default": "cpp",
					"enum": [
						"cpp",
						"java",
						"python",
						"python3",
						"c",
						"csharp",
						"javascript",
						"typescript",
						"php",
						"swift",
						"kotlin",
						"dart",
						"golang",
						"ruby",
						"scala",
						"rust",
						"racket",
						"erlang",
						"elixir"
					],
					"enumDescriptions": [
						"C++",
						"Java",
						"Python",
						"Python 3",
						"C",
						"C#",
						"JavaScript",
						"TypeScript",
						"PHP",
						"Swift",
						"Kotlin",
						"Dart",
						"Go",
						"Ruby",
						"Scala",
						"Rust",
						"Racket",
						"Erlang",
						"Elixir"
					],
					"description": "Default programming language for solutions"
				},
				"vs-leet.showTopicTags": {
					"type": "boolean",
					"default": true,
					"description": "Show topic tags in problem descriptions"
				},
				"vs-leet.workspaceFolder": {
					"type": "string",
					"default": "",
					"description": "Custom workspace folder path for solutions"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "vscode-test",
		"compile": "node esbuild.config.mjs",
		"compile:production": "node esbuild.config.mjs --production",
		"watch": "node esbuild.config.mjs --watch",
		"vscode:prepublish": "npm run build:web && npm run compile:production",
		"build:web": "cd web && npm install && npm run build",
		"package": "vsce package",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "22.x",
		"@types/vscode": "^1.90.0",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2",
		"autoprefixer": "^10.4.21",
		"esbuild": "^0.27.2",
		"eslint": "^9.36.0",
		"postcss": "^8.5.6",
		"sharp": "^0.34.5",
		"tailwindcss": "^3.4.18"
	},
	"dependencies": {
		"better-sqlite3": "^12.6.2",
		"dotenv": "^17.2.3",
		"node-fetch": "^3.3.2",
		"open": "^11.0.0",
		"playwright": "^1.56.0",
		"puppeteer": "^24.36.0",
		"puppeteer-extra": "^3.3.6",
		"puppeteer-extra-plugin-adblocker": "^2.13.6",
		"puppeteer-extra-plugin-stealth": "^2.11.2",
		"winston": "^3.18.3"
	}
}
