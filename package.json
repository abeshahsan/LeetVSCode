{
  "name": "webview",
  "displayName": "webview",
  "description": "",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "webview.leetcode",
        "title": "Leetcode"
      },
      {
        "command": "webview.openWebview",
        "title": "Come on"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Webview Extension",
      "properties": {
        "webview.autoShow": {
          "type": "boolean",
          "default": true,
          "description": "Automatically show the webview when VS Code starts. Set to false to disable automatic opening."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "playwright": "^1.56.0"
  }
}
